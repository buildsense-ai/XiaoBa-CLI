# Copy this file to: tenants/<tenant>/.env
# Each tenant should use its own credentials and quota limits.

# LLM
GAUZ_LLM_PROVIDER=anthropic
GAUZ_LLM_API_BASE=https://api.anthropic.com
GAUZ_LLM_API_KEY=replace-me
GAUZ_LLM_MODEL=claude-sonnet-4-20250514

# Tool safety
GAUZ_TOOL_ALLOW=write_file,edit_file

# Feishu (recommended: one tenant = one app/bot)
FEISHU_APP_ID=replace-me
FEISHU_APP_SECRET=replace-me
FEISHU_BOT_OPEN_ID=replace-me
FEISHU_BOT_ALIASES=小八,xiaoba

# Memory (optional)
GAUZ_MEM_ENABLED=false
GAUZ_MEM_BASE_URL=http://your-mem-server:1235
GAUZ_MEM_PROJECT_ID=XiaoBa
GAUZ_MEM_USER_ID=tenant-user
GAUZ_MEM_AGENT_ID=XiaoBa

# Vision (optional but recommended for CAD/paper image analysis)
GAUZ_VISION_PROVIDER=anthropic
GAUZ_VISION_API_BASE=https://api.anthropic.com
GAUZ_VISION_API_KEY=replace-me
GAUZ_VISION_MODEL=claude-sonnet-4-20250514

# MinerU + MinIO (optional for paper parser)
MINERU_TOKEN=replace-me
MINIO_ENDPOINT=replace-me:9000
MINIO_ACCESS_KEY=replace-me
MINIO_SECRET_KEY=replace-me
MINIO_BUCKET=replace-me
